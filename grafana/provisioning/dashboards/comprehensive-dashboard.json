      "title": "üéØ E2E Test Metrics & System Health"
    },
    {
      "id": 2,
      "title": "E2E Test Success Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sdv_e2e_test_success",
          "legendFormat": "Success"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "value": "0",
              "text": "FAILED"
            },
            {
              "type": "value",
              "value": "1",
              "text": "SUCCESS"
            }
          ],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 1
      }
    },
